                <span id="acte-dep">Departament acte auto</span> <span style="display:inline-block;"> <i class="fa fa-envelope mr-3"></i> <a href="mailto:acte.autoconsult@gmail.com ">acte.autoconsult@gmail.com</a></span> <br />
                <span style="margin-left:43%"> <i class="fa fa-phone mr-3"></i> <a href="tel:0769 325 555">+40 (0)769 325 555</a></span><br />
                <span style="margin-left:43%"> <i class="fa fa-phone mr-3"></i> <a href="tel:0769 666 088">+40 (0)769 666 088</a></span>
            </li>

        </ul>
        <ul class="nav navbar-nav col">
            <li>
                <iframe src="https://www.facebook.com/plugins/share_button.php?href=http://www.reparatiisitransport.ro/&layout=button&size=large&mobile_iframe=true&width=83&height=28&appId" width="83" height="28" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true"></iframe>
            </li>

        </ul>
    </nav>
    <nav class="navbar main-nav" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

        </div>
        <a class="navbar-brand" href="@Url.Action("Index", "Home", null)"><img src="~/Content/img/rp3.png" alt="Moba Tehnic firma de transporturi marfa " /></a>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="navbar-collapse-1">

            <ul class="nav navbar-nav navbar-left">
                <li>@Html.ActionLink("Acasa", "Index", "Home")</li>
                <li>@Html.ActionLink("Despre noi", "Firma_de_transport_reparatii", "AboutUs")</li>
                <li>@Html.ActionLink("Birou acte auto", "Acte_auto_inmatriculare_impozite_taxe", "Documents")</li>
                <li>@Html.ActionLink("I.T.P.", "Itp_inspectie_tehnica_periodica_craiova", "Itp")</li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li>@Html.ActionLink("Transport intern si international", "Transport_marfa_intern_international", "Transport")</li>
                <li>@Html.ActionLink("Reparatii", "Firma_reparatii_auto_masini", "Repairs")</li>
                <li>@Html.ActionLink("Cariere", "Cariere_sofer_reparatii_craiova", "Jobs")</li>
                <li>@Html.ActionLink("Contact", "Companie_de_transport_reparatii_contact", "Contact")</li>
            </ul>
        </div><!-- /.navbar-collapse -->
    </nav>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
